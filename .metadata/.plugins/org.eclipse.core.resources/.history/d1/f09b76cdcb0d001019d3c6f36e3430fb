package javatrenlop;
import java.util.Scanner;

public class cau1kiemtra {
public static int uc(int a, int b) {
	if(b==0) {
		return a;
	}
	else {
		return uc(b,a%b);
	}
}
public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
int a=sc.nextInt();
int b=sc.nextInt();
System.out.print(uc(a,b));
}
}
